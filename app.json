{
  "expo": {
    "name": "EcoReminder",
    "slug": "EcoReminder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/trash-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/trash-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.elevatic20.EcoReminder"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/trash-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/trash-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6273257e-e19d-483e-baa8-66a06a81d9b7"
      }
    }
  }
}
